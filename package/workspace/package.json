{
  "name": "@package/workspace",
  "version": "1.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "postinstall": "tsup"
  },
  "devDependencies": {
    "dotenv": "^17.2.3"
  },
  "peerDependencies": {
    "@package/common": "workspace:*"
  }
}