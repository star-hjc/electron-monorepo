{
  "name": "@package/wasm",
  "version": "1.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "pretask": "pnpm dev",
    "dev": "wasm-pack build --out-dir ./dist --out-name index --target web",
    "build": "wasm-pack build --release --out-dir ./dist --out-name index --target web"
  },
  "devDependencies": {
    "wasm-pack": "0.13.1"
  },
  "private": true
}