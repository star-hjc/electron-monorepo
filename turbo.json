{
    "tasks": {
        "pretask": {
            "cache": true,
            "persistent": false,
            "outputs": [
                "dist/**"
            ]
        },
        "@electron/renderer#pretask": {
            "dependsOn": [
                "cli#pretask"
            ]
        },
        "@electron/main#pretask": {
            "dependsOn": [
                "cli#pretask"
            ]
        },
        "cli#pretask": {
            "dependsOn": [
                "@package/workspace#pretask"
            ]
        },
        "@package/workspace#pretask": {
            "dependsOn": [
                "@package/common#pretask"
            ]
        },
        "@electron/preload#pretask": {
            "outputs": [
                "../main/dist/src/preload"
            ],
            "dependsOn": [
                "@package/electron#pretask"
            ]
        },
        "lint": {
            "cache": true,
            "persistent": false
        },
        "dev": {
            "cache": true,
            "persistent": true,
            "dependsOn": [
                "^dev"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "cli#dev": {
            "persistent": false,
            "outputs": [
                "bin/**"
            ]
        },
        "@package/bridge#dev": {
            "persistent": false,
            "outputs": [
                "dist/**",
                "libs/**"
            ]
        },
        "@package/wasm#dev": {
            "persistent": false,
            "outputs": [
                "dist/**"
            ]
        },
        "@electron/preload#dev": {
            "persistent": false,
            "outputs": [
                "dist/**"
            ]
        },
        "@electron/main#dev": {
            "dependsOn": [
                "@package/bridge#dev",
                "@package/wasm#dev"
            ]
        },
        "@electron/renderer#dev": {
            "dependsOn": [
                "@package/wasm#dev"
            ]
        },
        "build": {
            "cache": false,
            "persistent": false,
            "dependsOn": [
                "^build"
            ]
        },
        "@electron/main#build": {
            "cache": false,
            "persistent": false,
            "dependsOn": [
                "@electron/renderer#build"
            ]
        },
        "preview": {
            "cache": false,
            "persistent": true
        }
    }
}