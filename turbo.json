{
    "tasks": {
        "dev": {
            "cache": true,
            "persistent": true,
            "dependsOn": [
                "^dev"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "cli#dev": {
            "persistent": false,
            "outputs": [
                "bin/**"
            ]
        },
        "@package/bridge#dev": {
            "persistent": false,
            "outputs": [
                "dist/**",
                "libs/**"
            ]
        },
        "@package/wasm#dev": {
            "persistent": false,
            "outputs": [
                "dist/**"
            ]
        },
        "@electron/main#dev": {
            "dependsOn": [
                "@package/bridge#dev",
                "@package/wasm#dev"
            ]
        },
        "@electron/renderer#dev": {
            "dependsOn": [
                "@package/wasm#dev"
            ]
        },
        "build": {
            "cache": false,
            "persistent": false,
            "dependsOn": [
                "^build"
            ]
        },
        "@electron/main#build": {
            "cache": false,
            "persistent": false,
            "dependsOn": [
                "@electron/renderer#build"
            ]
        },
        "preview": {
            "cache": false,
            "persistent": true
        }
    }
}