{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "esModuleInterop": true,
        "baseUrl": "./src",
        "moduleResolution": "bundler",
        "outDir": "dist",
        "noImplicitAny": false,
        "resolveJsonModule": true,
        "paths": {
            "@/*": [
                "./*"
            ],
        }
    },
    "include": [
        "src/**/*",
        "tsup.config.ts",
        "pretask.ts"
    ]
}