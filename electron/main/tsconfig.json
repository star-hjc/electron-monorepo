{
    "compilerOptions": {
        "module": "esnext",
        "esModuleInterop": true,
        "baseUrl": "./src",
        "moduleResolution": "bundler",
        "outDir": "dist",
        "noImplicitAny": false,
        "resolveJsonModule": true,
        "paths": {
            "@/config/*": ["./config/*"],
            "@/logger": ["./logger/index"],
            "@/ipc": ["./ipc/index"],
            "@/windows": ["./windows/index"],
            "@/windows/*": ["./windows/*"],
        }
    }
}